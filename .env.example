# .env file example
# This file contains environment variables loaded by RepDev

# Database
DB_HOST=localhost
DB_PORT=5432
DB_NAME=myapp_dev
DB_USER=postgres
DB_PASSWORD=secret123

# API Keys
API_KEY=your_api_key_here
JWT_SECRET=your_jwt_secret_here

# Application
NODE_ENV=development
PORT=3000
LOG_LEVEL=debug

# Feature Flags
ENABLE_FEATURE_X=true
MAX_UPLOAD_SIZE=10485760

# External Services
REDIS_URL=redis://localhost:6379
MONGODB_URI=mongodb://localhost:27017/myapp
